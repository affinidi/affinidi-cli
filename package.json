{
  "name": "@affinidi/cli",
  "version": "2.0.0-beta.68",
  "description": "Affinidi Command Line Interface",
  "author": "affinidi.com",
  "license": "Apache-2.0",
  "typings": "dist/index.d.ts",
  "bin": {
    "affinidi": "./bin/run"
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "dev": "./bin/dev",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "mocha \"test/unit/**/*.test.ts\"",
    "test:integration": "exit 0",
    "checkLicenses": "license-checker --production --failOn 'AGPL 3.0;LGPL 2.1'",
    "generate:readme": "npm run build && oclif readme",
    "generate:api": "node scripts/generate-api.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/affinidi/foundational/phoenix/affinidi-cli.git"
  },
  "keywords": [
    "affinidi",
    "cli",
    "oclif"
  ],
  "dependencies": {
    "@inquirer/confirm": "^2.0.10",
    "@inquirer/prompts": "^3.1.1",
    "@inquirer/select": "^1.2.9",
    "@oclif/core": "^2.13.0",
    "@oclif/plugin-autocomplete": "^2.3.6",
    "@oclif/plugin-commands": "^2.2.25",
    "@oclif/plugin-help": "^5.2.18",
    "@oclif/plugin-not-found": "^2.4.1",
    "@oclif/plugin-search": "^0.0.22",
    "@oclif/plugin-warn-if-update-available": "^2.0.49",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "conf": "^10.2.0",
    "express": "^4.18.2",
    "fs-extra": "^9.1.0",
    "http-status-codes": "^2.2.0",
    "open": "^8.4.1",
    "pem-jwk": "^2.0.0",
    "simple-oauth2": "^5.0.0",
    "tcp-port-used": "^1.0.2",
    "zod": "^3.22.2",
    "zod-validation-error": "^1.5.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.8",
    "@types/chai": "4.3.5",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "10.0.1",
    "@types/node": "^18.11.19",
    "@types/pem-jwk": "^2.0.0",
    "@types/simple-oauth2": "^5.0.4",
    "@types/tcp-port-used": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.9",
    "chai": "4.3.7",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "license-checker": "25.0.1",
    "mocha": "^10.2.0",
    "nyc": "15.1.0",
    "oclif": "^3.9.0",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "swagger-typescript-api": "^12.0.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "affinidi",
    "dirname": "affinidi",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-search"
    ],
    "topicSeparator": " ",
    "topics": {
      "project": {
        "description": "Use these commands to manage your projects"
      },
      "login": {
        "description": "Use these commands for user login configuration and group management"
      },
      "token": {
        "description": "Use these commands for token management to call Affinidi API's programmatically"
      },
      "iam": {
        "description": "Use these commands to manage policies for access configuration"
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 7
    },
    "hooks": {
      "check": "./dist/hooks/check/check-version"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "dist/index.d.ts"
}
